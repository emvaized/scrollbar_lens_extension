{
    "manifest_version": 2,
    "name": "Lens Scrollbar",
    "version": "0.0.1",
    "author": "emvaized",
    "description": "View the whole page at once and jump to any part of it immediately",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "index.js"
            ],
            "css": [
                "index.css"
            ],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "<all_urls>"
    ],
    "browser_specific_settings": {
        "gecko": {
          "id": "lens_scrollbar@emvaized.dev"
        }
    }
}